{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "esnext",
    "target": "esnext",
    "outDir": "../build/common",
    "moduleResolution": "Bundler",
    "strict": true,
    "allowJs": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "composite": true
  },
  "include": ["*.ts"]
}
